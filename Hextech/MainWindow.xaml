<Window x:Class="Hextech.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hextech"
        xmlns:vm="clr-namespace:Hextech.ViewModels"
        mc:Ignorable="d"
        Title="Hextech" Height="576" Width="844">
    
    <Window.DataContext>
        <vm:MainWindowViewModel x:Name="vm"/>
    </Window.DataContext>

    <Frame Content="{Binding Content}" NavigationUIVisibility="Hidden" Navigating="Frame_Navigating"/>
</Window>
